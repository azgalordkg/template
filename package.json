{
  "name": "nextjs-boilerplate",
  "version": "1.0.0",
  "description": "NextJS boilerplate with TypeScript and Redux",
  "main": "index.js",
  "scripts": {
    "test": "NODE_ENV=test jest",
    "test:dev": "NODE_ENV=test jest --watchAll",
    "start": "NODE_ENV=production PORT=$PORT next start",
    "build": "rm -rf .next && next build",
    "dev": "next dev --port=4000",
    "export": "npm run build && next export",
    "prettier": "prettier  --write **/*.{ts,tsx,js}",
    "lint:ts": "tslint -c tslint.json -t stylish --fix {pages,src,server}/**/*.{ts,tsx}",
    "lint:scss": "stylelint --fix {src,pages}/**/*.scss",
    "lint:types": "tsc -p . --noEmit",
    "storybook": "start-storybook -s ./public -p 6006",
    "build-storybook": "build-storybook"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint:types && lint-staged"
    }
  },
  "engines": {
    "node": "^12.19.0"
  },
  "engineStrict": true,
  "lint-staged": {
    "*.{ts,tsx}": [
      "npm run prettier",
      "npm run lint:ts"
    ],
    "*.scss": [
      "npm run lint:scss"
    ]
  },
  "keywords": [
    "nextjs",
    "boilerplate",
    "typeccript",
    "react",
    "redux",
    "prettier",
    "stylelint",
    "jest",
    "enzyme",
    "svg sprite",
    "husky",
    "lint staged"
  ],
  "author": "Sanjay Twisk",
  "repository": {
    "type": "git",
    "url": "git@github.com:sanjaytwisk/nextjs-ts.git"
  },
  "license": "ISC",
  "dependencies": {
    "@hookform/resolvers": "1.0.0",
    "@types/classnames": "^2.2.11",
    "@types/leaflet": "^1.5.19",
    "@types/leaflet-providers": "^1.2.0",
    "@types/leaflet.markercluster": "^1.4.3",
    "@types/react-dom": "^17.0.0",
    "@types/react-image-gallery": "^1.0.1",
    "@types/react-input-mask": "^3.0.0",
    "@types/react-js-pagination": "^3.0.3",
    "@types/react-responsive": "^8.0.2",
    "@types/react-slick": "^0.23.4",
    "@types/yup": "^0.29.11",
    "antd": "^4.9.1",
    "antd-img-crop": "^3.14.1",
    "axios": "^0.21.0",
    "immutable": "^4.0.0-rc.12",
    "next": "9.5.5",
